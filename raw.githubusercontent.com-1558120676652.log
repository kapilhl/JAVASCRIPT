function Student(rollno,name,phy,math,chem){
this.rollno=rollno;
this.name=name;
this.phy=phy;
this.math=math;
this.chem=chem;
this.total=function(){
var sum=this.phy+this.math+this.chem;
return sum;
}

Student.prototype.percent=function(){
   return ((this.total())/3);
} 
Student.prototype.grade=function(){ 
  if((this.total()/3)>=90)
     console.log("GRADE OF "+this.name+" IS: A1");
  else if((this.total()/3)>=80 && (this.total()/3)<90)
console.log("GRADE OF "+this.name+" IS: A");
else if((this.total()/3)>=70 && (this.total()/3)<80)
console.log("GRADE OF "+this.name+" IS: B");
else if((this.total()/3)>=60 && (this.total()/3)<70)
console.log("GRADE OF "+this.name+" IS: C");
else
console.log("GRADE OF "+this.name+" IS: D");
}
Student.prototype.report=function(){
console.log("TOTAL OF "+this.name+" IS: "+this.total());
console.log("PERCENTAGE OF "+this.name+" IS: "+this.percent());
console.log(this.grade());
}}
